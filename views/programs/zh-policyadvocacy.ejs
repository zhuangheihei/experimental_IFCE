</div>
<hr>


<div class = "container">
    <div class = "row">
        <% programs.forEach(function(program){ %>
            <div class = "col-md-12 col-sm-12">
                <div>
                    <a href="/zh/programs/<%= program._id %>" style = "display:inline-block;">
                        <h5><%= program.title %></h5>
                    </a>
                </div>
            </div>
        <% }); %>
    </div>
</div>

